{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "",
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "roots": ["<rootDir>/src"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest.setup-tests.js",
    "testPathIgnorePatterns": ["<rootDir>/node_modules"],
    "coveragePathIgnorePatterns": ["<rootDir>/config"]
  },
  "scripts": {
    "prod":
      "cross-env NODE_ENV=production BABEL_ENV=production rimraf dist && webpack --config config/webpack.config.prod.js -p",
    "test-deploy": "node scripts/test-deploy",
    "start": "npm run dev",
    "dev":
      "cross-env NODE_ENV=development BABEL_ENV=development webpack-dev-server --config config/webpack.config.dev.js --hot --history-api-fallback --open",
    "test": "cross-env NODE_ENV=testing BABEL_ENV=testing jest",
    "test:watch": "cross-env NODE_ENV=testing BABEL_ENV=testing jest --watch",
    "test:coverage":
      "cross-env NODE_ENV=testing BABEL_ENV=testing jest --coverage",
    "env": "cross-env NODE_ENV=testing BABEL_ENV=testing node scripts/env.js",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "express": "^4.16.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "open": "0.0.5",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react-cosmos": "^3.2.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
